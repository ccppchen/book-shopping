-if(msg!=null) {
-var cls = msgcode===0?'alert-danger':'alert-success'
  div.messages-container
    div.alert.alert-dismissible.messages(role="alert",class='#{cls}')
      button.close(data-dismiss="alert" aria-label="Close")
        span(aria-hidden="true") &times;
      != msg
  script(src='/javascripts/msgAnimate.js')
-}

